{
  "manifest_version": 3,
  "name": "chrome-extension-typescript-boilerplate",
  "description": "chrome-extension-typescript-boilerplate",
  "version": "1.0",
  "icons": {
    "16": "icons/logo.png",
    "32": "icons/logo.png",
    "48": "icons/logo.png",
    "128": "icons/logo.png"
  },
  "content_scripts": [
    {
      "js": ["content-script.js"],
      "matches": ["http://*.example.com//"]
    }
  ],
  "action": {
    "default_title": "Fake Lotto Naver",
    "default_icon": "icons/logo.png",
    "default_popup": "popup.html"
  }
}
